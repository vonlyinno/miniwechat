.icon-up {
  background-image: url('data:image/png;base64,');
  background-size: 70%;
  background-repeat: no-repeat;
  background-position: center;
}

.icon-down {
  background-image: url('data:image/png;base64,');
  background-size: 70%;
  background-repeat: no-repeat;
  background-position: center;
}

.icon-plus {
  background-image: url('data:image/png;base64,');
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center;
  width: 94rpx;
  height: 44rpx;
  display: inline-block;
}

.block__item-type-text {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  background-image: url('data:image/png;base64,');
  background-size: 100%;
  background-repeat: no-repeat;
}

.block__item-type-image {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  background-image: url('data:image/png;base64,');
  background-size: 100%;
  background-repeat: no-repeat;
}

.block {
  position: relative;
  margin-top: 1.17647059em;
}

.block .block__item {
  margin-top: 0;
}

.block__toolbox {
  position: absolute;
  right: 0;
  top: 5px;
  z-index: 100;
}

.block__toolbox .icon {
  display: inline-block;
  margin-right: 5px;
}

.block__item>.weui-cell {
  padding: 0;
}

.block__item .weui-cell:before {
  left: 0;
}

.block__sidebar .icon {
  margin: 0 auto;
}

.padding {
  padding: 15px 0 !important;
}

.u-textarea {
  height: 100px;
  width: 98%;
  display: block;
  padding: 25px 2% 15px;
  z-index: 1;
}

.block__btns {
  margin-top: 20px;
}

.u-image {
  width: 100%;
  display: block;
}

.weui-panel__bd {
  padding: 15px 10px;
}

.submitBtn {
  width: 320rpx;
  height: 90rpx;
  background-color: transparent;
  border: none;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}
.submitBtn::after{
  border: none;
}
.weui-cells{
  background-color: transparent;
}
.weui-cell-white{
  background-color:#fff;
}
.weui-cells-noborder:after,.weui-cells-noborder:before{
  border: none;
}
.weui-cell-noborder:before,.weui-cell-noborder:after{
  border: none;
}
