.content {
  z-index: 1;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: rgba(0,0,0,0.80);
  letter-spacing: 0;
}

.icon {
  width: 52rpx;
  height: 52rpx;
}

