/* pages/writeConfirm.wxss */
.page {
  font-family: HanziPenSC-W3;
  font-size: 20px;
  color: rgba(0,0,0,0.70);
  letter-spacing: 0;
}
.icon {
  width: 30px;
  height: 30px;
  margin-right: 5px;
}
.slider{
  margin-left: 80px;
  margin-right: 80px;
  margin-top: 580rpx;
}
.submitBtn {
  width: 320rpx;
  height: 90rpx;
  background-color: transparent;
  border: none;
  background-repeat: no-repeat;
  margin-bottom:15px;
  margin-top:40rpx;
  font-family: PingFangSC-Regular;
font-size: 18px;
color: rgba(90,31,0,0.80);
  background-image: url(data:image/png;base64,);
}
.submitBtn::after{
  border: none;
}
.cancelBtn {
  width: 320rpx;
  height: 90rpx;
  background-color: transparent;
  border: none;
  font-family: PingFangSC-Regular;
font-size: 18px;
color: rgba(90,31,0,0.80);
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,);
}
.cancelBtn::after{
  border: none;
}
.postmanPic{
  position: absolute;
  width: 343rpx;
  height: 343rpx;
  align-items: center;
  margin-top:150rpx;
  left:calc(50% - 171.5rpx);
}
.postmanChoose{
position: absolute;
text-align:center;
margin-top:80rpx;
font-size:52rpx;
width:100%;
color: rgba(90,31,0,0.80);

}
.postmanPrompt{
position: absolute;
text-align:center;
font-size:40rpx;
width:100%;
margin-top: 500rpx;
color: rgba(0,0,0,0.70);
}
.weui-cells{
  background-color: transparent;
}
.weui-cell-white{
  background-color:#fff;
}
.weui-cells-noborder:after,.weui-cells-noborder:before{
  border: none;
}
.weui-cell-noborder:before,.weui-cell-noborder:after{
  border: none;
}
.weui-cell-edit-line {
  position: absolute;
  bottom:10rpx;
  right: 30rpx;
  width: 398rpx;
  
}
.write_info {
  position:relative;
  width:626rpx;
  margin:0 auto;
}
.write_info input {
  width: 396rpx;
  padding-left: 15rpx;
}
.weui-select {
  border-right: none;
  padding-left: 20rpx;
}

